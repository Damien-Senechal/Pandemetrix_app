{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --ci --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "test:unit": "jest --testPathPattern=tests/unit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run --headless",
    "test:e2e:dev": "cypress open",
    "test:e2e:ci": "cypress run --headless --reporter junit --reporter-options mochaFile=cypress-results.xml",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:all:watch": "npm run test:watch & npm run test:e2e:dev",
    "test:coverage:open": "npm run test:coverage && open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.12",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "install": "^0.13.0",
    "tailwindcss": "^4.0.12",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@cypress/vite-dev-server": "^5.0.7",
    "@cypress/vue": "^6.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.3",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.2",
    "cypress-axe": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "plotly.js": "^3.0.1",
    "vite": "^6.2.0"
  }
}
